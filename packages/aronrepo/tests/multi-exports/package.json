{
    "name": "multi-exports",
    "main": "./dist/index.cjs",
    "jsnext:main": "./dist/index.mjs",
    "browser": "./dist/index.browser.js",
    "esnext": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "node": {
                "require": "./dist/index.cjs",
